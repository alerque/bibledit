{
  "manifest_version": 2,

  "name": "Bibledit extension open url",
  "description": "This extension open a URL in a new tab",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Bibledit"
  },
  "permissions": [
    "activeTab"
  ],
  "background":
  {
    "scripts": ["background.js"]
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  }
}
